{
  "name": "florenttorregrosa/docker-drupal-project",
  "description": "Drupal project template with Docker environment.",
  "type": "project",
  "homepage": "https://github.com/FlorentTorregrosa/docker-drupal-project",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Florent Torregrosa",
      "homepage": "https://github.com/FlorentTorregrosa",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/FlorentTorregrosa/docker-drupal-project/issues",
    "source": "https://github.com/FlorentTorregrosa/docker-drupal-project",
    "docs": "https://github.com/FlorentTorregrosa/docker-drupal-project/blob/master/README.md"
  },
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    "drush_language": {
      "type": "vcs",
      "url": "git@github.com:dawehner/drush_language.git"
    },
    "d3": {
      "type": "package",
      "package": {
        "name": "d3/d3",
        "version": "v3.5.17",
        "type": "drupal-library",
        "source": {
          "url": "https://github.com/d3/d3",
          "type": "git",
          "reference": "v3.5.17"
        }
      }
    },
    "highlightjs": {
      "type": "package",
      "package": {
        "name": "components/highlightjs",
        "version": "9.7.0",
        "type": "drupal-library",
        "source": {
          "url": "https://github.com/components/highlightjs",
          "type": "git",
          "reference": "9.7.0"
        }
      }
    }
  },
  "require": {
    "composer/installers": "^1.0.21",
    "cweagans/composer-patches": "~1.0",
    "drupal/core": "8.2.4",
    "drush/drush": "8.1.8",
    "drupal/drush_language": "dev-8.x-1.x",
    "drupal/admin_toolbar": "1.18.0",
    "drupal/elasticsearch_connector": "2.*",
    "drupal/features": "3.2.0",
    "drupal/piwik": "1.1.0",
    "drupal/redis": "1.0.0-alpha1",
    "drupal/search_api": "1.0.0-beta3",
    "drupal/search_api_solr": "1.0.0-beta1"
  },
  "require-dev": {
    "drupal/config_inspector": "1.*",
    "drupal/devel": "1.*",
    "drupal/devel_a11y": "1.*",
    "drupal/examples": "1.*",
    "drupal/renderviz": "1.*",
    "drupal/search_kint": "1.*",
    "drupal/pistachio": "1.*",
    "d3/d3": "3.5.*",
    "components/highlightjs": "9.7.*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-install-cmd": [
      "bash ../scripts/update-scaffold.sh",
      "bash ../scripts/update-libraries.sh"
    ]
  },
  "extra": {
    "installer-paths": {
      "core": ["type:drupal-core"],
      "modules/contrib/{$name}": ["type:drupal-module"],
      "themes/contrib/{$name}": ["type:drupal-theme"],
      "profiles/contrib/{$name}": ["type:drupal-profile"],
      "libraries/{$name}": ["type:drupal-library"],
      "../drush/contrib/{$name}": ["type:drupal-drush"]
    },
    "patches": {
      "drupal/redis": {
        "Currently Drush Cr or Cache Clear UI does not flush Redis cache": "https://www.drupal.org/files/issues/redis-drush_cr-2765895-4.patch"
      }
    }
  }
}
