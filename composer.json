{
  "name": "florenttorregrosa/docker-drupal-project",
  "description": "Drupal project template with Docker environment.",
  "type": "project",
  "homepage": "https://github.com/FlorentTorregrosa/docker-drupal-project",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Florent Torregrosa",
      "homepage": "https://github.com/FlorentTorregrosa",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/FlorentTorregrosa/docker-drupal-project/issues",
    "source": "https://github.com/FlorentTorregrosa/docker-drupal-project",
    "docs": "https://github.com/FlorentTorregrosa/docker-drupal-project/blob/master/README.md"
  },
  "repositories": {
    "asset-packagist": {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/7"
    }
  },
  "require": {
    "composer/installers": "^1.0.21",
    "cweagans/composer-patches": "~1.0",
    "drupal-composer/drupal-l10n": "^1.0",
    "drupal-composer/preserve-paths": "0.1.*",
    "drupal/admin_menu": "3.0.0-rc6",
    "drupal/drupal": "7.63.0",
    "drupal/l10n_update": "2.2.0",
    "drupal/redis": "3.17.0",
    "drupal/search_api": "1.25.0",
    "drupal/search_api_solr": "1.14.0",
    "drupal/views": "3.20.0",
    "drush/drush": "8.1.18",
    "oomphinc/composer-installers-extender": "^1.1"
  },
  "require-dev": {
    "drupal/coder": "8.3.*",
    "drupal/devel": "1.*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "platform": {
      "php": "7.2"
    }
  },
  "scripts": {
    "post-create-project-cmd": [
      "bash ./scripts/init.sh"
    ]
  },
  "extra": {
    "installer-types": [
      "bower-asset",
      "npm-asset"
    ],
    "installer-paths": {
      "www/": [
        "type:drupal-core"
      ],
      "www/sites/all/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "www/sites/all/modules/custom/{$name}": [
        "type:drupal-custom-module"
      ],
      "www/sites/all/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "www/sites/all/themes/custom/{$name}": [
        "type:drupal-custom-theme"
      ],
      "www/sites/all/libraries/{$name}": [
        "type:drupal-library",
        "type:bower-asset",
        "type:npm-asset"
      ],
      "www/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "drush/contrib/{$name}": [
        "type:drupal-drush"
      ]
    },
    "preserve-paths": [
      "www/sites/all/modules/contrib",
      "www/sites/all/modules/custom",
      "www/sites/all/themes/contrib",
      "www/sites/all/themes/custom",
      "www/sites/all/libraries",
      "www/profiles/contrib",
      "www/sites/default",
      "www/sites/development.settings.php"
    ],
    "drupal-l10n": {
      "destination": "sites/all/translations/contrib",
      "languages": [
        "fr"
      ]
    },
    "patches": {
      "drupal-composer/preserve-paths": {
        "Add pre-install command event regarding composer-patches": "https://github.com/SebCorbin/composer-preserve-paths/commit/88d7f376453307331353c2c2c834c7b51826f149.patch"
      }
    }
  }
}
