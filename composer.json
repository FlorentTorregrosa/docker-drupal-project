{
    "name": "florenttorregrosa/docker-drupal-project",
    "description": "Drupal project template with Docker environment.",
    "type": "project",
    "homepage": "https://github.com/FlorentTorregrosa/docker-drupal-project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Florent Torregrosa",
            "homepage": "https://github.com/FlorentTorregrosa",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/FlorentTorregrosa/docker-drupal-project/issues",
        "source": "https://github.com/FlorentTorregrosa/docker-drupal-project",
        "docs": "https://github.com/FlorentTorregrosa/docker-drupal-project/blob/master/README.md"
    },
    "repositories": {
        "d3": {
            "type": "package",
            "package": {
                "name": "d3/d3",
                "version": "v3.5.17",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "d3"
                },
                "dist": {
                    "url": "https://github.com/d3/d3/archive/v3.5.17.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        },
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "drupal-l10n": {
            "type": "vcs",
            "url": "https://github.com/FlorentTorregrosa/drupal-l10n"
        },
        "highlightjs": {
            "type": "package",
            "package": {
                "name": "components/highlightjs",
                "version": "9.12.0",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "highlightjs"
                },
                "dist": {
                    "url": "https://github.com/components/highlightjs/archive/9.12.0.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        }
    },
    "require": {
        "composer/installers": "^1.0.21",
        "cweagans/composer-patches": "~1.0",
        "drupal-composer/drupal-l10n": "dev-master",
        "drupal-composer/drupal-scaffold": "^2.2",
        "drupal/admin_toolbar": "1.23.0",
        "drupal/core": "8.5.0",
        "drupal/drush_language": "1.0.0-rc1",
        "drupal/piwik": "1.2.0",
        "drupal/purge": "3.0.0-beta8",
        "drupal/redis": "1.0.0-rc2",
        "drupal/search_api": "1.7.0",
        "drupal/search_api_solr": "2.0.0-alpha3",
        "drupal/varnish_purge": "1.10.0",
        "drush/drush": "9.2.1"
    },
    "require-dev": {
        "anam/phantomjs-linux-x86-binary": "~2.1.1",
        "components/highlightjs": "9.12.*",
        "d3/d3": "3.5.*",
        "drupal/coder": "8.2.*",
        "drupal/config_inspector": "1.*",
        "drupal/console": "1.*",
        "drupal/devel": "1.*",
        "drupal/devel_a11y": "1.*",
        "drupal/drupal-extension": "3.*",
        "drupal/examples": "1.*",
        "drupal/pistachio": "1.*",
        "drupal/renderviz": "1.*",
        "drupal/search_kint": "1.*",
        "drupal/speedboxes": "1.*",
        "webflo/drupal-core-require-dev": "~8.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "platform": {
            "php": "7.1"
        }
    },
    "scripts": {
        "drupal-l10n": "DrupalComposer\\DrupalL10n\\Plugin::download",
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "post-create-project-cmd": [
            "bash ./scripts/init.sh"
        ],
        "post-install-cmd": [
            "@drupal-scaffold"
        ],
        "post-update-cmd": [
            "@drupal-scaffold"
        ]
    },
    "extra": {
        "installer-paths": {
            "www/core": [
                "type:drupal-core"
            ],
            "www/modules/contrib/{$name}": [
                "type:drupal-module",
                "type:drupal-drush"
            ],
            "www/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "www/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "www/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "www/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "www/libraries/{$name}": [
                "type:drupal-library"
            ]
        },
        "drupal-scaffold": {
            "excludes": [
                ".ht.router.php",
                "update.php",
                "web.config",
                "sites/development.services.yml",
                "sites/example.settings.local.php"
            ]
        },
        "drupal-l10n": {
            "destination": "translations/contrib",
            "languages": [
                "fr"
            ]
        },
        "patches": {
            "drupal/config_inspector": {
                "Filter entities with schema errors": "https://www.drupal.org/files/issues/filter_entities_with-2843729-7.patch"
            },
            "drupal/redis": {
                "Add status page and some statistics": "https://www.drupal.org/files/issues/redis-statistics-7.patch"
            }
        }
    }
}
